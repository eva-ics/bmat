{
  "name": "bmat",
  "version": "0.1.18",
  "type": "module",
  "files": [
    "*"
  ],
  "types": "./bmat.d.ts",
  "main": "./bmat.cjs",
  "module": "./bmat.js",
  "exports": {
    "./dashtable": {
      "import": "./dashtable.js",
      "require": "./dashtable.cjs"
    },
    "./dom": {
      "import": "./dom.js",
      "require": "./dom.cjs"
    },
    "./hooks": {
      "import": "./hooks.js",
      "require": "./hooks.cjs"
    },
    "./log": {
      "import": "./log.js",
      "require": "./log.cjs"
    },
    "./numbers": {
      "import": "./numbers.js",
      "require": "./numbers.cjs"
    },
    "./time": {
      "import": "./time.js",
      "require": "./time.cjs"
    },
    "./tools": {
      "import": "./tools.js",
      "require": "./tools.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc src/*.ts --skipLibCheck --allowImportingTsExtensions --declaration --jsx react-jsx --lib es2020,dom,es2021.String --emitDeclarationOnly --declarationDir dist/ && cp ./d/bmat.d.ts dist/ && cp package.json dist/"
  },
  "keywords": [
    "browser",
    "framework"
  ],
  "author": "Bohemia Automation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eva-ics/bmat/issues"
  },
  "homepage": "https://github.com/eva-ics/bmat/",
  "peerDependecies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "mathjs": "^12.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.2",
    "mathjs": "^12.3.2",
    "vite": "^4.4.5"
  }
}
